                                        "data": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/users/getUserDetail": {
            "get": {
                "tags": [
                    "USER"
                ],
                "summary": "Method used to get user details by id",
                "parameters": [
                    {
                        "name": "language",
                        "default": "en",
