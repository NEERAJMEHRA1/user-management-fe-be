    "paths": {
        "/users/userRegister": {
            "post": {
                "tags": [
                    "USER"
                ],
                "summary": "Method used to register",
                "security": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "contentType"
                                ],
                                "properties": {
                                    "language": {
                                        "type": "string",
                                        "default": "en"
                                    },
                                    "firstName": {
                                        "type": "string",
                                        "default": "Neeraj"
                                    },
                                    "lastName": {
                                        "type": "string",
                                        "default": "Mehra"
                                    },
                                    "email": {
                                        "type": "string",
                                        "default": "test@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "default": "****"
                                    },
                                    "address": {
                                        "type": "string",
                                        "default": "Indore MP"
                                    },
                                    "age": {
                                        "type": "number",
                                        "default": "23"
                                    },
                                    "gender": {
                                        "type": "string",
                                        "default": "male/female"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/users/userLogin": {
            "post": {
                "tags": [
                    "USER"
                ],
                "summary": "Method used login user",
                "security": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "contentType"
                                ],
                                "properties": {
                                    "language": {
                                        "type": "string",
                                        "default": "en"
                                    },
                                    "email": {
                                        "type": "string",
                                        "default": "test@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "default": "12345678"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean"
