            "post": {
                "tags": [
                    "USER"
                ],
                "summary": "Method used to get all user list with filter and paginations",
                "security": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "contentType"
                                ],
                                "properties": {
                                    "language": {
                                        "type": "string",
                                        "default": "en"
                                    },
                                    "search": {
                                        "type": "string",
                                        "default": "Neeraj Mehra"
                                    },
                                    "page": {
                                        "type": "number",
                                        "default": 1
                                    },
                                    "perPage": {
                                        "type": "number",
                                        "default": 10
